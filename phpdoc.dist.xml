<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>Fluent Validator</title>
  <parser>
    <target>phpdocs/output</target>
    <extensions>
      <extension>php</extension>
    </extensions>
  </parser>
  <transformer>
    <target>phpdocs/docs</target>
  </transformer>
  <files>
    <file>src/*</file>
    <file>src/*/*</file>
    <ignore>tests/*</ignore>
    <ignore>Tests/*</ignore>
  </files>
</phpdoc>